{
  "name": "@torian12321/js-library-template",
  "version": "0.0.1",
  "type": "module",
  "prettier": "@torian12321/prettier-config",
  "scripts": {
    "build": "tsc && vite build",
    "format": "eslint --fix",
    "format:check": "eslint",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "prepublishOnly": "npm run build && npm run test"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torian12321/js-library-template.git"
  },
  "main": "./dist/js-library-template.js",
  "module": "./dist/js-library-template.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/js-library-template.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@torian12321/eslint-config": "0.0.7",
    "@torian12321/prettier-config": "0.0.2",
    "@torian12321/typescript-config": "0.0.1",
    "@types/node": "^20.19.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
